{
  "name": "forestry",
  "version": "0.4.1",
  "description": "A lightweight javascript library for creating and manipulating trees.",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamcdonald/forestry.git"
  },
  "bugs": "https://github.com/iamcdonald/forestry/issues",
  "main": "src/Forestry.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "nyc --reporter=lcov npm run test:src && nyc report",
    "test:src": "babel-tape-runner 'test/src/**/*.spec.js' | tap-spec",
    "test:lib": "babel-tape-runner 'test/lib/**/*.spec.js' | tap-spec",
    "test:dist": "browserify test/dist/Forestry.spec.js -t [ babelify --presets [ es2015 stage-0 ] ] | tap-closer | smokestack | tap-spec",
    "test:dist-min": "browserify test/dist/Forestry.min.spec.js -t [ babelify ] | tap-closer | smokestack | tap-spec",
    "browser-test": "browserify $TEST -t [ babelify ] | double-tap smokestack -b $BROWSER -s -u $USER -k $KEY | tap-spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist lib",
    "build:lib": "babel src -d lib && npm run test:lib",
    "build:dist": "rollup -c -i ./src/Forestry.js && npm run test:dist",
    "minify:dist": "uglifyjs --compress --mangle -- dist/Forestry.js > dist/Forestry.min.js && npm run test:dist-min",
    "build": "npm run test:src && npm run clean && npm run build:lib && npm run build:dist && npm run minify:dist"
  },
  "keywords": [
    "tree",
    "node"
  ],
  "author": {
    "name": "Iain McDonald",
    "email": "iain.allan.mcdonald@googlemail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.9.0",
    "babel-eslint": "6.0.0-beta.6",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-proto-to-assign": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "babelify": "^7.2.0",
    "babelify-external-helpers": "^1.1.0",
    "browserify": "^7.0.0",
    "coveralls": "^2.11.6",
    "derequire": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-xo-space": "^0.10.0",
    "eslint-plugin-babel": "^3.1.0",
    "istanbul": "^0.3.17",
    "jshint": "^2.5.2",
    "mocha": "^2.2.4",
    "nyc": "^5.5.0",
    "proxyquire": "^1.6.0",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "sinon": "^1.15.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "uglify-js": "^2.4.16"
  },
  "dependencies": {
    "babel-runtime": "^6.9.0"
  }
}
